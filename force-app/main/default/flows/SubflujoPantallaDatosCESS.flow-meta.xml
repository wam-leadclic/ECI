    <assignments>
        <description>Asigna el valor del AccountId en la variable AccountId</description>
        <name>AsignaInputAccountId</name>
        <label>Asigna inputAccountId</label>
        <locationX>50</locationX>
        <locationY>518</locationY>
        <assignmentItems>
            <assignToReference>AccountId</assignToReference>
            <operator>Assign</operator>
