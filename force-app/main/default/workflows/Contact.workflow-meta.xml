<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>ClientContactedByAgent</fullName>
        <description>Cliente quiere ser contactado por un agente</description>
        <protected>false</protected>
        <recipients>
            <field>Email</field>
            <type>email</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>CESS/TemplateContactAgent</template>
    </alerts>
    <alerts>
        <fullName>ClientNotConctedByAgent</fullName>
        <description>Cliente no quiere ser contactado por un agente</description>
        <protected>false</protected>
        <recipients>
            <field>Email</field>
            <type>email</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>CESS/TemplateNotContactAgent</template>
    </alerts>
</Workflow>
