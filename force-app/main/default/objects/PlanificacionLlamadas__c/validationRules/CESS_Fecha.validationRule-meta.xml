<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CESS_Fecha</fullName>
    <active>true</active>
    <errorConditionFormula>CESS_Date__c  
	 &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>CESS_Date__c</errorDisplayField>
    <errorMessage>La fecha debe de ser igual o mayor al fecha de hoy</errorMessage>
</ValidationRule>
